# creates a panel of all the random images
convert -size 2000x2000 xc:white -draw "image SrcOver 0,0 500,500, 'img/hiveplot.1.png'" -draw "image SrcOver 500,0 500,500, 'img/hiveplot.2.png'" -draw "image SrcOver 1000,0 500,500, 'img/hiveplot.3.png'" -draw "image SrcOver 1500,0 500,500, 'img/hiveplot.4.png'" -draw "image SrcOver 0,500 500,500, 'img/hiveplot.5.png'" -draw "image SrcOver 500,500 500,500, 'img/hiveplot.6.png'" -draw "image SrcOver 1000,500 500,500, 'img/hiveplot.7.png'" -draw "image SrcOver 1500,500 500,500, 'img/hiveplot.8.png'" -draw "image SrcOver 0,1000 500,500, 'img/hiveplot.9.png'" -draw "image SrcOver 500,1000 500,500, 'img/hairball.png'" img/panel.png
